{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "node": false,
        "mocha": false
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-const-assign": "error",
        "no-this-before-super": "warn",
        "no-unreachable": "warn",
        "constructor-super": "warn",
        "valid-typeof": "warn",
        "no-throw-literal": "warn"
    }
}
